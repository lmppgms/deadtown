if mouse_check_button_pressed(mb_left)
{
    if mouse_x &gt; 230
    {
    if mouse_y &gt; 64 + 110
    {
    if mouse_y &lt; room_height-64
    {
        drag = 1
        drag_start_x = mouse_x
        drag_start_drag_x = drag_x
    }
    }
    }
}

if instance_exists(par_popup)
{
    drag = 0
}

if mouse_check_button(mb_left)
{
    if drag
    {
        drag_x = drag_start_drag_x + (mouse_x - drag_start_x)
        drag_speed = (mouse_x - drag_start_x)/2
        
        if drag_speed &gt; drag_speed_max {drag_speed = drag_speed_max}
        if drag_speed &lt; -drag_speed_max {drag_speed = -drag_speed_max}
    }
}
else
{
    drag = 0
}

if mouse_check_button_released(mb_left)
{
    drag = 0
}

if drag == 0
{
    drag_x += drag_speed
    if drag_speed &gt; 0
    {
        if (drag_speed &gt; drag_firction) {drag_speed -= drag_firction} else {drag_speed = 0}
    }
    if drag_speed &lt; 0
    {
        if (drag_speed &lt; -drag_firction) {drag_speed += drag_firction} else {drag_speed = 0}
    }
}

if drag_x &gt; 0
{
    drag_x = 0
}

drag_max = -max(0,floor((item_number-6+1)/2)*(240+30))
if drag_x &lt; drag_max
{
    drag_x = drag_max
}
