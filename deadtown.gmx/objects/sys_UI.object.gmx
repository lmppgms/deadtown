        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// pause
/*
0 : normal
1 : ads
2 : death
*/

if os_is_paused()
{
    if !pause
    {
        if backpack_open == 0
        {
            pause_request = 1
            pause_kind = 0
        }
    }
}

if ads_watching
{
    exit;
}

for (i=0;i&lt;5;i++)
{
    if device_mouse_check_button_released(i,mb_left)
    {
        if point_distance(display_get_width()-pause_gap,pause_gap,device_mouse_raw_x(i),device_mouse_raw_y(i))&lt;pause_radius
        {
            if pause == 1
            {
                if pause_kind == 0
                {
                    pause_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
            }
            else
            {
                pause_request = 1
                pause_kind = 0
                audio_play_sound(snd_button_click,0,0)
                break;
            }
        }
        
        if pause == 1
        {
            if pause_kind == 0
            {
                if abs(device_mouse_raw_x(i) - display_get_width()/2 - ui_paused_1_x) &lt; ui_paused_button_width/2
                if abs(device_mouse_raw_y(i) - display_get_height()/2 - ui_paused_1_y) &lt; ui_paused_button_height/2
                {
                    global.save_request = 1
                    global.save_after = 1
                    pause_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
                if abs(device_mouse_raw_x(i) - display_get_width()/2 - ui_paused_2_x) &lt; ui_paused_button_width/2
                if abs(device_mouse_raw_y(i) - display_get_height()/2 - ui_paused_2_y) &lt; ui_paused_button_height/2
                {
                    global.save_request = 1
                    global.save_after = 2
                    pause_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
                if abs(device_mouse_raw_x(i) - display_get_width()/2 - ui_paused_3_x) &lt; ui_paused_button_width/2
                if abs(device_mouse_raw_y(i) - display_get_height()/2 - ui_paused_3_y) &lt; ui_paused_button_height/2
                {
                    global.save_request = 1
                    global.save_after = 3
                    pause_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
                if abs(device_mouse_raw_x(i) - display_get_width()/2 - ui_paused_4_x) &lt; ui_paused_button_width/2
                if abs(device_mouse_raw_y(i) - display_get_height()/2 - ui_paused_4_y) &lt; ui_paused_button_height/2
                {
                    pause_request = 1
                    global.save_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
            }
            xx1 = display_get_width()/2 - 100*global.gui_aspect
            yy1 = display_get_height()/2 + 70*global.gui_aspect
            xx2 = display_get_width()/2 + 100*global.gui_aspect
            yy2 = display_get_height()/2 + 70*global.gui_aspect
            if pause_kind == 1
            {
                if abs(device_mouse_raw_x(i)-xx1)&lt;60*global.gui_aspect and abs(device_mouse_raw_y(i)-yy1)&lt;30*global.gui_aspect // ads okay
                {
                    if unity_ads_check_is_can_show()
                    {
                        unity_ads_show_scr()
                        ads_watching = 1
                        ads_type = 0
                    }
                    else
                    {
                        pause_request = 1
                    }
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
                if abs(device_mouse_raw_x(i)-xx2)&lt;60*global.gui_aspect and abs(device_mouse_raw_y(i)-yy2)&lt;30*global.gui_aspect // ads cancel
                {
                    pause_request = 1
                    audio_play_sound(snd_button_click,0,0)
                    break;
                }
            }    
            if pause_kind == 2
            {
                if abs(device_mouse_raw_x(i)-xx1)&lt;60*global.gui_aspect and abs(device_mouse_raw_y(i)-yy1)&lt;30*global.gui_aspect // rebirth okay
                {
                    if rebirth_price(global.player_death_count) &lt;= global.money
                    {
                        audio_play_sound(snd_button_click,0,0)
                        
                        instance_create(0,0,obj_player_rebirth)
                        
                        global.money -= rebirth_price(global.player_death_count)
                        shop_inven_save()
                        
                        global.player_death_count += 1
                        pause_request = 1
                        break;
                    }
                }
                if abs(device_mouse_raw_x(i)-xx2)&lt;60*global.gui_aspect and abs(device_mouse_raw_y(i)-yy2)&lt;30*global.gui_aspect // rebirth cancel
                {
                    audio_play_sound(snd_button_click,0,0)
                    
                    leaderboard_post(0,global.player_survival_day)
                    leaderboard_post(1,global.player_level)
                    
                    if file_exists(global.save_file_name)
                    {
                        file_delete(global.save_file_name)
                    }
                    room_goto(rom_lobby)
                    break;
                }
                
                if device_mouse_raw_x(i) &gt; display_get_width() - 280*global.gui_aspect
                {
                    if device_mouse_raw_y(i) &lt; 50*global.gui_aspect
                    {
                        room_goto(rom_shop)
                        global.goto_shop = 1
                        break;
                    } 
                }
            }
        }
    }
}

if pause_request
{
    pause_request = 0
    if pause == 0
    {
        pause = 1
        particle_pause()
        audio_pause_all();
        instance_deactivate_all(1)
        instance_activate_object(sys_global)
    }
    else
    {
        pause = 0
        instance_activate_all()
        deactive_auto()
        particle_play()
        audio_resume_all();
        
        sys_view.left_press = -1
        sys_view.right_press = -1
        
        sys_view.right_button = 0
        sys_view.right_button_pressed = 0
        sys_view.right_button_released = 0
        
        sys_view.left_direction = 0
        sys_view.left_distance = 0
        sys_view.left_distance_ratio = 0
    }
}
