            <kind>1</kind>
            <string>/// control
draw_skip = 0
if backpack_open == 1
{
    ///---------------------- drag ---------------------- ///
    for (i=0;i&lt;5;i++)
    {
        if device_mouse_check_button_pressed(i,mb_left)
        {
            if drag &lt; 0
            {
                drag = i
                drag_start_x = device_mouse_raw_x(i)
                drag_start_y = device_mouse_raw_y(i)
                drag_before_y = device_mouse_raw_y(i)
                drag_start_backpack_x = backpack_x
                drag_start_backpack_y = backpack_y
            }
        }
        
        if device_mouse_check_button(i,mb_left)
        {
            if drag == i
            {
                backpack_x = drag_start_backpack_x+(device_mouse_raw_x(i)-drag_start_x)*drag_sensitive
                
                drag_speed = (device_mouse_raw_y(i)-drag_before_y)/2*drag_sensitive/1.2
                backpack_y = drag_start_backpack_y+(device_mouse_raw_y(i)-drag_start_y)*drag_sensitive/1.2
                
                if drag_speed &gt; drag_speed_max {drag_speed = drag_speed_max}
                if drag_speed &lt; -drag_speed_max {drag_speed = -drag_speed_max}
                
                drag_before_y = device_mouse_raw_y(i)
            }
        }
        else
        {
            if drag == i
            {
                drag = -1
            }
        }
        
        if device_mouse_check_button_released(i,mb_left)
        {
            if drag == i
            {
                drag = -1
            }
        }
    }
    
    if drag == -1
    {
        backpack_y += drag_speed
        if drag_speed &gt; 0
        {
            if (drag_speed &gt; drag_firction) {drag_speed -= drag_firction} else {drag_speed = 0}
        }
        if drag_speed &lt; 0
        {
            if (drag_speed &lt; -drag_firction) {drag_speed += drag_firction} else {drag_speed = 0}
        }
    }
    
    ///---------------------- limit left ---------------------- ///
    if backpack_menu_number &lt;= 0
    {
        if (backpack_x &lt; 0)
