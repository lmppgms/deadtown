            <string>/// location check install
if object != noone
{
    if instance_exists(obj_player)
    {
        sp = object_get_sprite(object)
        ms = object_get_mask(object)
        sprite_index = sp
        mask_index = ms
        
        length = sprite_get_height(sp)
        dir = sys_view.view_a+90
        
        x = obj_player.x + lengthdir_x(length,dir)
        y = obj_player.y + lengthdir_y(length,dir)
        
        aa = place_meeting(x,y,par_solid) == 0 and place_meeting(x,y,par_zombie) == 0 and place_meeting(x,y,par_action) == 0
        bb = place_meeting(x,y,obj_trap) == 0 and place_meeting(x,y,obj_gps) == 0 and place_meeting(x,y,par_action_door) == 0
        
        if aa and bb
        {
            install_able = 1
        }
        else
        {
            install_able = 0
        }
        
        if install_able
        {
            if sys_view.action_button_pressed
            {
                achievement_plus(0)
                
                if object == obj_turret or object == obj_lightaction or object == obj_bettery
                {achievement_plus(1)}
                
                sys_view.action_button_pressed = 0
                ins = instance_create(x,y,object)
                ins.image_angle = image_angle
                ins.craft_light = craft_light
                ins.need_bettery = need_bettery
                instance_destroy()
            }
        }
        
        
        image_angle = sys_view.view_a
    }
}
